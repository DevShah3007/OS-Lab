diff -ruN xv6-public/cpuProcTester.c xv6-public-without-hybrid/cpuProcTester.c
--- xv6-public/cpuProcTester.c	1970-01-01 05:30:00.000000000 +0530
+++ xv6-public-without-hybrid/cpuProcTester.c	2022-10-07 15:04:28.678966270 +0530
@@ -0,0 +1,55 @@
+#include "types.h"
+#include "stat.h"
+#include "user.h"
+
+int useless = 0;
+int
+main(int argv, char* argc[]){
+
+	if(argv<2){
+		printf(1, "Incorrect format!\nFormat: test_cpu_only <number of fork processes>\n");
+
+		exit();
+	}
+
+	int n=atoi(argc[1]);
+
+	int burst_times[n];
+
+	for(int i=0;i<n;i++){
+		burst_times[i] = randomrange(1,20);
+	}
+
+	printf(1, "\tPID \t Type \t Burst Time \t Context Switches\n");
+	printf(1, "\t___ \t ____ \t __________ \t ________________\n\n");
+	for(int i=0;i<n;i++){
+		if(!fork()){
+
+			// CPU Bound process
+			set_burst_time(burst_times[i]);
+
+			int it=0;
+			for(int j=0;j<100;j++){
+				for(int k=0;k<1000000;k++){
+					if((j+k)%2)it++;
+					else it--;
+				}
+			}
+			useless+=it;
+			
+			struct processInfo info;
+			getCurrentInfo(&info);
+			int bt = get_burst_time();
+			int currID = getCurrentPID();
+			int ns = info.numberContextSwitches;
+			printf(1, "\t%d \t CPU         %d                 %d\n", currID, bt, ns);
+			exit();
+
+
+		}
+	}
+
+	while(wait()!=-1);
+	exit();
+
+}
