+    /* [KNUTH 1981, The Art of Computer Programming */
+    /*    Vol. 2 (2nd Ed.), pp102]                  */
+    mt[0]= seed & 0xffffffff;
+ 76b:	89 15 80 0d 00 00    	mov    %edx,0xd80
+    for (mti=1; mti<N; mti++)
