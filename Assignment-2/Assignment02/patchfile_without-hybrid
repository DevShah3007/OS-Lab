diff -ruN xv6-public/ioProcTester.c xv6-public-without-hybrid/ioProcTester.c
--- xv6-public/ioProcTester.c	1970-01-01 05:30:00.000000000 +0530
+++ xv6-public-without-hybrid/ioProcTester.c	2022-10-06 14:32:25.744395466 +0530
@@ -0,0 +1,50 @@
+#include "types.h"
+#include "stat.h"
+#include "user.h"
+
+int
+main(int argv, char* argc[]){
+
+	if(argv<2){
+		printf(1, "Incorrect format!\nFormat: test_cpu_only <number of fork processes>\n");
+
+		exit();
+	}
+
+	int n=atoi(argc[1]);
+
+	int burst_times[n];
+
+	for(int i=0;i<n;i++){
+		burst_times[i] = randomrange(1,20);
+	}
+
+	printf(1, "\tPID \t Type \t Burst Time \t Context Switches\n");
+	printf(1, "\t___ \t ____ \t __________ \t ________________\n\n");
+	for(int i=0;i<n;i++){
+		if(!fork()){
+
+			// CPU Bound process
+			set_burst_time(burst_times[i]);
+
+			for(int i=0;i<20;i++){
+				sleep(1);
+			}
+			
+			struct processInfo info;
+			getCurrentInfo(&info);
+			int bt = get_burst_time();
+			if(bt<10)
+				printf(1, "\t%d \t I/O         %d                  %d\n", getCurrentPID(), bt, info.numberContextSwitches);
+			else
+				printf(1, "\t%d \t I/O         %d                 %d\n", getCurrentPID(), bt, info.numberContextSwitches);
+			exit();
+
+
+		}
+	}
+
+	while(wait()!=-1);
+	exit();
+
+}
