+static void
+printint(int fd, int xx, int base, int sgn)
+{
+ 590:	55                   	push   %ebp
+ 591:	89 e5                	mov    %esp,%ebp
+ 593:	57                   	push   %edi
+ 594:	56                   	push   %esi
+ 595:	53                   	push   %ebx
+ 596:	83 ec 3c             	sub    $0x3c,%esp
+ 599:	89 4d c4             	mov    %ecx,-0x3c(%ebp)
+  uint x;
+
+  neg = 0;
+  if(sgn && xx < 0){
+    neg = 1;
+    x = -xx;
+ 59c:	89 d1                	mov    %edx,%ecx
+{
+ 59e:	89 45 b8             	mov    %eax,-0x48(%ebp)
+  if(sgn && xx < 0){
