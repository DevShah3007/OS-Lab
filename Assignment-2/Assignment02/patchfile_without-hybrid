-			current_proc->ofile[file] = 0;
+
+  //Check all the files opened by the current process and close them one by one by setting the reference to NULL
+	for(fd = 0;fd<NOFILE;fd++){
+		if(curproc->ofile[fd]){
+			fileclose(curproc->ofile[fd]);
+			curproc->ofile[fd] = 0;
 		}
 	}
