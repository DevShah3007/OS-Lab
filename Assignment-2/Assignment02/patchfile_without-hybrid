+ 8c3:	8d 74 26 00          	lea    0x0(%esi,%eiz,1),%esi
+ 8c7:	90                   	nop
+ 8c8:	83 c0 04             	add    $0x4,%eax
+        mt[mti] = (69069 * mt[mti-1]) & 0xffffffff;
+ 8cb:	69 d2 cd 0d 01 00    	imul   $0x10dcd,%edx,%edx
+ 8d1:	89 10                	mov    %edx,(%eax)
